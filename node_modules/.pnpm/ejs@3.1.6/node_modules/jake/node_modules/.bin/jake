#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules/jake/bin/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules/jake/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules:/mnt/e/Minor Pojects/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules/jake/bin/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules/jake/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/jake@10.8.2/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules/.pnpm/node_modules:/mnt/e/Minor Pojects/School Management System/node_modules:/mnt/e/Minor Pojects/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
